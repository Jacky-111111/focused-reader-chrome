  "manifest_version": 3,
  "name": "Focused Reader",
  "version": "1.0.0",
  "description": "Bionic-style reading for better focus (ADHD-friendly)",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Focused Reader"
  }
}
